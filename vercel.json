{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.mjs"
    }
  ],
  "env": {
    "NODE_VERSION": "22",
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
    "PUPPETEER_SKIP_DOWNLOAD": "true"
  },
  "buildCommand": "npm install --ignore-scripts && npm run build:vercel",
  "outputDirectory": "dist",
  "installCommand": "npm install --ignore-scripts"
}